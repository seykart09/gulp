    {% set modalContent %}
    <div class="card border-0 p-0">
      <div class="modal-body">
        <div class="mb-4">
          <h5 class="modal-title font-weight-bold" id="exampleModalLabel">Report an issue here</h5>
          <p class="mb-0">Report your issue at hand as clearly as possible</p>
        </div>
        <div>
          <div class="mb-3">
            <label for="exampleFormControlInput1" class="form-label mb-1">Category</label>
            <select class="form-select" aria-label="Default select example">
              <option value="1" selected>Messaging</option>
              <option value="2">Payments</option>
              <option value="3">Customers</option>
            </select>
          </div>
          <div class="mb-3">
            <label for="exampleFormControlInput1" class="form-label mb-1">Issues</label>
            <select class="form-select" aria-label="Default select example">
              <option value="1" selected>Messaging not sending</option>
              <option value="2">Payments failing</option>
            </select>
          </div>
          <div class="mb-3">
            <label for="exampleFormControlTextarea1" class="form-label mb-1">Tell us what happened</label>
            <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="Enter your issue"></textarea>
          </div>
          <div>
            <label for="exampleFormControlTextarea1" class="form-label mb-1">Upload screenshots</label>
            <div class="image-upload-wrap">

              <div class="pb-0 pt-2 px-2 images images-0 align-items-center">

                <div class="pointer pic" id="images-0" title="Click to upload"></div>
              </div>

            </div>
          </div>
        </div>
      </div>
      <div class="modal-footer border-0">
        <a href="#" data-bs-dismiss="modal" class="btn btn-outline-primary">Back</a>
        <a href="#" data-bs-dismiss="modal" data-bs-target="#reportSubmitted" data-bs-toggle="modal" class="btn btn-primary">Submit</a>
      </div>
    </div>
    {% endset %}

