    <script>
      var count = 1;

      uploadImage(count);

      function uploadImage(count) {
        var imageCounter = count - 1;
        var button = $(".images-" + imageCounter + " .pic");
        var uploader = $('<input type="file" accept="image/*" multiple/>');
        var images = $('#images-' + imageCounter);
        var container = $('.media-upload');
        var imgholder = $('.img');

        button.on('click', function () {
          uploader.click();
        })

        uploader.on('change', function () {
          var reader = new FileReader();
          for (var i = 0; i < uploader[0].files.length; ++i) {
            var reader = new FileReader();
            reader.onload = function (event) {
              images.before('<div class="img" style="background-image: url(\'' + event.target.result + '\');" rel="' + event.target.result + '"></div>');
            }
            reader.readAsDataURL(uploader[0].files[i]);
          }
        });

      }
    </script>

